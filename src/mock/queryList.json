{
	"/boms/api/billReceipt/add": {
		"request": {
			"queryVO": {
				"addBillReceiptItems": {
					"isArray": true,
					"amount": {
						"type": "number",
						"example": 2020.1,
						"description": "金额"
					},
					"chargingMode": {
						"type": "integer",
						"format": "int32",
						"example": 1,
						"description": "收费模式(字典：boms_receiptsChargingMode)"
					},
					"costProject": {
						"type": "integer",
						"format": "int32",
						"example": 101,
						"description": "费用项目ID(字典：boms_receiptsCostProject)"
					},
					"isDeduct": {
						"type": "integer",
						"format": "int32",
						"example": 1,
						"description": "是否抵扣（1是，2否）(字典：boms_isDeduct)"
					},
					"remark": {
						"type": "string",
						"example": "备注",
						"description": "备注"
					},
					"serviceRate": {
						"type": "number",
						"example": 0.1025,
						"description": "服务费率（0.1025代表10.25%）"
					}
				},
				"billDictId": {
					"type": "integer",
					"format": "int64",
					"example": 202,
					"description": "账单名称字典ID"
				},
				"costDuring": {
					"type": "string",
					"example": "2020-10",
					"description": "费用期间(例如2020-10)"
				},
				"receiptsDate": {
					"type": "string",
					"example": "2018-10-01",
					"description": "单据日期"
				},
				"remark": {
					"type": "string",
					"example": "备注",
					"description": "备注"
				},
				"supplyerId": {
					"type": "string",
					"example": "1245",
					"description": "供应商ID"
				}
			}
		},
		"response": {
			"code": {
				"type": "string"
			},
			"currentTimeMillis": {
				"type": "integer",
				"format": "int64"
			},
			"data": {
				"type": "boolean"
			},
			"msg": {
				"type": "string"
			},
			"success": {
				"type": "boolean"
			}
		}
	}
}